{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true, 
    "allowJs": true,
    "jsx":"preserve"
  },
  "typeRoots": [
      "../node_modules/@types/"
  ],
  "noImplicitAny": false,
  "include": [
    "env.d.ts",
    ".nuxt/nuxt.d.ts",
    "**/*"
  ]
}
